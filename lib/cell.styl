cell($cell = flex, $gutter = 'inside')
  if $cell == flex
    flex 1
  else if unit($cell) == '' and $cell >= 1 and $cell <= $num-cells
    $perc = 100% / ($num-cells / $cell)
    flex 0 0 $perc
  else if unit($cell) == '' and $cell > $num-cells
    error('' + $cell + ' is not a accepted value.')
  else
    flex 1 0 $cell

  if $gutter == 'inside'
    gutter-inside()
  else
    gutter-outside()
